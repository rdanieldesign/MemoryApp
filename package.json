{
  "name": "MemoryApp",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run serve & npm run watchify",
    "serve": "node ./bin/www",
    "bundle": "browserify -t [ babelify --presets [ react ] ] public/javascripts/main.js -o public/javascripts/bundle.js",
    "watchify": "watchify -t [ babelify --presets [ react ] ] public/javascripts/main.js -o public/javascripts/bundle.js",
    "build": "npm run bundle"
  },
  "dependencies": {
    "babel-preset-react": "^6.3.13",
    "babelify": "^7.2.0",
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "jade": "~1.11.0",
    "mongodb": "^2.1.0",
    "monk": "^1.0.1",
    "morgan": "~1.6.1",
    "node-sass": "^3.4.2",
    "node-sass-middleware": "^0.9.7",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "serve-favicon": "~2.3.0",
    "watchify": "^3.6.1"
  },
  "devDependencies": {
    "mongodb": "^2.1.0"
  }
}
